{
    "name": "laravel-admin-ext/cropper",
    "description": "Integrate Cropper into laravel-admin, removed dependencies",
    "type": "library",
    "keywords": ["laravel-admin", "extension","cropper","image"],
    "homepage": "https://github.com/axsweet/cropper",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex"
        }
    ],
    "require": {
        "php": "*",
        "encore/laravel-admin": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Cropper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Cropper\\CropperServiceProvider"
            ]

        }
    }
}
